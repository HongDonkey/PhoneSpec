<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.com.PhoneSpec.Galaxy.dao.SamsungDeviceMapper">
  <select id="getRecentGalaxy" resultType="com.com.PhoneSpec.Galaxy.dto.SamsungDeviceDto">
    select productnm from SAMSUNG_DEVICE
	ORDER BY SEQ DESC
	LIMIT 3;
  </select>
</mapper>